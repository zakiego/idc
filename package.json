{
  "name": "indonesia-code",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "description": "A simple library to get Indonesia's province, city, and district code",
  "keywords": ["indonesia", "code", "province", "city", "district"],
  "author": {
    "name": "Zakiego",
    "url": "https://zakiego.com"
  },
  "scripts": {
    "build": "bunchee --no-external",
    "pull": "NODE_TLS_REJECT_UNAUTHORIZED=0 tsx src/data/pull.ts",
    "test": "vitest",
    "release:add": "changeset",
    "release:version": "changeset version",
    "release:publish": "changeset publish"
  },
  "files": ["dist"],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^20.11.16",
    "bunchee": "^4.4.6",
    "node-fetch": "^3.3.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2",
    "@changesets/cli": "^2.27.1"
  },
  "dependencies": {
    "zod": "^3.22.4"
  }
}
